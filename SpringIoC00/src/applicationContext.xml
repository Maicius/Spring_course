<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="userDAO_A" class="com.neusoft.withspring.UserDAO_A" />
	<bean id="userDAO_B" class="com.neusoft.withspring.UserDAO_B" />
	
	<bean id="userService" class="com.neusoft.withspring.UserService">
		<property name="userDAO" ref="userDAO_B" />
	</bean>
</beans>